{
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 3,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "vue",
    "svelte",
    "swift",
    "ruby"
